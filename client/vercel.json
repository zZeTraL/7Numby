{
  "framework": "vite",
  "git": {
    "deploymentEnabled": {
      "master": true
    }
  },
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "routes": [
    { "src": "/src/(.*)", "dest": "/src/$1" }
  ]
}